<a href="#n/<%= id %>" class="<%= unread ? 'unread' : 'read' %>">
  <div><%= reason %></div>
  <span class="octicon octicon-<%= subject.octicon %>"></span>
  <span class="title"><%- subject.title %></span>
  <span class="repository"><%- repository.owner.login %> / <%- repository.name %></span>
  <time class="js-relative-time" datetime="<%= updated_at %>"><%= updated_at %></time>
</a>
